{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prefer-regex-literals": 0,
        "consistent-return":0,
        "import/no-extraneous-dependencies":0,
        "func-names":0,
        "no-underscore-dangle":0
    }
}